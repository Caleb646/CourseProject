{
  "manifest_version": 3,
  "name": "Webscraper",
  "version": "0.0.2",
  "description": "This is for the final project of CS410.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "action": {
    "default_popup": "pages/main.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ]

}
